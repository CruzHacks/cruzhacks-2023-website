    "@types/react": "^18.0.16",
    "@types/react-dom": "^18.0.0",
    "ansi-regex": "6.0.1",
    "axios": "^0.27.2",
    "babel-loader": "^8.2.5",
    "dotenv": "^16.0.3",
    "firebase": "^9.15.0",
    "firebase-tools": "13.0.0",
    "jest": "^28.1.3",
    "notistack": "^2.0.8",
    "open": "6.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-donut-chart": "^1.3.3",
    "react-google-recaptcha": "^2.1.0",
    "react-qr-code": "^2.0.11",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-router-dom": "6.5.0",
    "react-scripts": "5.0.1",
    "react-tsparticles": "1.40.2",
    "sass": "^1.54.0",
